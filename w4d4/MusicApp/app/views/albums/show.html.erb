<h1><%= @album.name %></h1>
<h2>By: <%= link_to "#{@album.band.name}", band_url(@album.band) %></h2>
<% @tracks.each do |track| %>
  <h3>
    <%= link_to "#{track.name}", track_url(track) %>
  </h3>
<% end %>

<%= link_to "New Track", new_album_track_url(@album) %>

<%= button_to "Delete", album_url(@album), method: :delete %>
